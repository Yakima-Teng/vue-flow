{
  "presets": [
    "flow",
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "stage-2"
  ],
  "plugins": [
    "transform-vue-jsx",
    "transform-runtime",

    "transform-class-properties",
    "transform-flow-strip-types",
    ["typecheck", {
      "disable": {
        "production": true
      }
    }]
  ]
}
